package javaapplication60;

import java.util.Arrays;

/**
 *
 * @author vaio
 */
public class JavaApplication60 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        String[] os = new String[] {"Mac", "Linux", "Windows"
    };
        Arrays.sort(os);
        System.out.println(Arrays.binarySearch(os,"RedHat"));
        
        /*
        There is no element “RedHat” in the array. 
        If it is, it would be between the second and third element. 
        If it is not, it takes the negative index. Therefore output is -3.
        */
    }   
}
