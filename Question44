
package q44;


public class Q44 {

    private boolean numLock = true;
    static boolean capLock = false;
    
    public static void main(String[] args) {
        System.out.print(numLock + " "+ capLock);
        /*
        The numLock variable is not accessible in the static main() method without
        an instance of the Keyboard class; therefore, the code does not compile.
        */
    }
